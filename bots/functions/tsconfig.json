{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@src/*": ["./src/*"],
      "@bots/*": ["./src/bots/*"],
      "@capsolvers/*": ["./src/capsolvers/*"]
    },
    "esModuleInterop": true
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}
